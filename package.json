{
  "name": "next-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "next start",
    "start:dev": "next dev",
    "start:test": "run-s populate-mock-orders-from-file start:prod",
    "start:prod": "run-p start start:jobs",
    "start:jobs": "ts-node --skip-project api/jobs/worker.ts --run-jobs",
    "test:ui": "cypress install && cypress run --component",
    "test:api": "jest --testPathPattern=tests/api",
    "test:api:ci": "jest --testPathPattern=tests/api --ci --runInBand --detectOpenHandles --forceExit",
    "test:api:watch": "jest --testPathPattern=tests/api --watch",
    "test:e2e": "cypress install && cypress run",
    "test:all": "cross-var wait-on http-get://$TEST_APP_URI/api && run-p test:api test:ui test:e2e",
    "test:all:ci": "cross-var wait-on http-get://$TEST_APP_URI/api && run-p test:api:ci test:ui test:e2e",
    "test:debug": "cypress open",
    "lint": "next lint",
    "populate-mock-orders-from-file": "ts-node --skip-project api/mocks/populate-mock-orders-from-file.ts",
    "populate-mock-orders": "ts-node --skip-project api/mocks/populate-mock-orders.ts",
    "create-mock-orders-file": "ts-node --skip-project api/mocks/create-mock-orders-file.ts"
  },
  "dependencies": {
    "@hokify/agenda": "^6.3.0",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.5",
    "classnames": "^2.3.2",
    "command-line-args": "^5.2.1",
    "d3": "^7.8.4",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "mongoose": "^7.0.1",
    "next": "13.2.3",
    "next-api-decorators": "^2.0.2",
    "numeral": "^2.0.6",
    "path-to-regexp": "^6.2.1",
    "pino": "^8.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "ts-node": "^10.9.1",
    "use-resize-observer": "^9.1.0"
  },
  "devDependencies": {
    "@cypress/snapshot": "^2.1.7",
    "@faker-js/faker": "^7.6.0",
    "@types/command-line-args": "^5.2.0",
    "@types/cypress": "^1.1.3",
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.1",
    "@types/node": "18.14.2",
    "@types/numeral": "^2.0.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "cross-var": "^1.1.0",
    "cypress": "^12.11.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.62.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4",
    "wait-on": "^7.0.1"
  }
}
